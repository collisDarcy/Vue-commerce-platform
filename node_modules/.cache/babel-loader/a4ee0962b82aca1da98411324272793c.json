->\r\n              <el-menu-item index=\"1-4-1\">\r\n                <template slot=\"title\">\r\n                  <!-- 图标 -->\r\n                  <i class=\"el-icon-location\"></i>\r\n                  <span slot=\"title\">导航一</span>\r\n                </template>\r\n              </el-menu-item>\r\n            </el-submenu>\r\n          </el-menu>\r\n        </el-aside>\r\n        <!-- 内容主体区域 -->\r\n        <el-main>Main</el-main>\r\n      </el-container>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      isCollapse: true,\r\n    }\r\n  },\r\n  methods: {\r\n    goback() {\r\n      // 清空本地存储的token\r\n      window.sessionStorage.clear()\r\n      // 强制跳转到登录页\r\n      this.$router.push('/login')\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#index-Page {\r\n  height: 700px;\r\n  border: 1px solid cyan;\r\n  background-color: red;\r\n}\r\n.el-header {\r\n  background-color: #373d41;\r\n  height: 100px !important;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding-left: 0px;\r\n  // align-items: center;\r\n  color: #eaedf1;\r\n  font-size: 20px;\r\n  font-family: cursive;\r\n  > div {\r\n    display: flex;\r\n    align-items: center;\r\n    span {\r\n      margin-left: 15px;\r\n    }\r\n    img {\r\n      height: 100%;\r\n      border: 1px dotted #438884;\r\n      border-radius: 10px;\r\n      box-shadow: 0 0 30px #438884;\r\n    }\r\n  }\r\n  .el-button--info {\r\n    background-color: cyan;\r\n    height: 50%;\r\n    // margin: 0 auto;\r\n    margin-top: 25px;\r\n  }\r\n}\r\n.el-aside {\r\n  background-color: #333744;\r\n}\r\n.el-main {\r\n  background-color: #eaedf1;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}